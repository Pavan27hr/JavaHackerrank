def breakingRecords(scores):
    count2=0
    count1=0
    m2=scores[0]
    m1=scores[0]
    for i in range(1,len(scores)):
      if(scores[i]>m1):
            m1=scores[i]
            count1=count1+1
       
      if(scores[i]<m2):
            m2=scores[i]
            count2=count2+1
    return [count1,count2
