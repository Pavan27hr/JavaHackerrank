def timeConversion(s):
    t=s[-2:]
    if(t=="AM"):
        if s[0:2]==12:
            return "01:"+s[3:8]
        else:
            return s[0:9]
    else:
        if(s[0:2]==12):
            return s[0:9]
        else:
            a = int(s[:2]) + 12
            return str(str(a) + s[2:8])
            
